	function getRoot(char)
		local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
		return rootPart
	end

	for _, v in pairs(game:GetService("Players"):GetChildren()) do
		if v.Character and v.Character:FindFirstChild("Christmas_Sock") == nil and v.Character:FindFirstChild("FULLY_LOADED_CHAR") and v.Character.BodyEffects['K.O'].Value == false and v ~= game:GetService("Players").LocalPlayer then

			while v.Character:FindFirstChild("Christmas_Sock") == nil and v.Character.BodyEffects['K.O'].Value == false do

				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop['[BrownBag] - $25'].Head.CFrame
				wait(.30)
				fireclickdetector(game.Workspace.Ignored.Shop['[BrownBag] - $25'].ClickDetector)
				game.Players.LocalPlayer.Backpack:WaitForChild("[BrownBag]").Parent = game.Players.LocalPlayer.Character


				pcall(function()

					getRoot(game.Players[v.Name].Character).CFrame = getRoot(game.Players.LocalPlayer.Character).CFrame + Vector3.new(1,3,0)
					game.Players.LocalPlayer.Character["[BrownBag]"]:Activate()
					getRoot(game.Players[v.Name].Character).CFrame = getRoot(game.Players.LocalPlayer.Character).CFrame + Vector3.new(1,3,0)

				end)

			end
		end
	end	
end

else
BagAllReal = false
end
	end)

local Toggle = Toggles:CreateToggle("Block Aura", "Block if someone hits near you", function(BlockAura) sound()
	
if BlockAura then
	
	BlockAuraReal = true

	while BlockAuraReal do wait()
		
		local MainEvent = game:GetService('ReplicatedStorage').MainEvent
		local Distancia = 15;
		
		local forbidden = {'[Popcorn]','[HotDog]','[GrenadeLauncher]','[RPG]','[SMG]','[TacticalShotgun]','[AK47]','[AUG]','[Glock]', '[Shotgun]','[Flamethrower]','[Silencer]','[AR]','[Revolver]','[SilencerAR]','[LMG]','[P90]','[DrumGun]','[Double-Barrel SG]','[Hamburger]','[Chicken]','[Pizza]','[Cranberry]','[Donut]','[Taco]','[Starblox Latte]','[BrownBag]','[Weights]','[HeavyWeights]'}
		local Found = false
		for _,v in pairs(game.Workspace.Players:GetChildren()) do
			if (v.UpperTorso.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= Distancia then
				if v.BodyEffects.Attacking.Value == true and not table.find(forbidden, v:FindFirstChildWhichIsA('Tool').Name ) and v.Name ~= player.Name then
					Found = true
					MainEvent:FireServer('Block', game.Players.LocalPlayer.Name)

				end
			end
		end
		if Found == false then
			if game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild('Block') then game.Players.LocalPlayer.Character.BodyEffects.Block:Destroy() end
		end
		
	end

else

	BlockAuraReal = false

end
end)

