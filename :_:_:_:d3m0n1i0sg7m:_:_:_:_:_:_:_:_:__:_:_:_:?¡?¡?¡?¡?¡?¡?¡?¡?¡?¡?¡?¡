wait(0.20)
end

wait(0.20)
		jo.Character.HumanoidRootPart.CFrame = CFrame.new(AX1B,AY1B,AZ1B)

break


		elseif game.Players[target].Character.BodyEffects['K.O'].Value == false and game.Players[target].Character.BodyEffects['Dead'].Value == false or game:GetService("Workspace").Players:WaitForChild(target):FindFirstChild("GRABBING_CONSTRAINT") == 'GRABBING_CONSTRAINT' then 
			
		
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TargetPiso_.Position + Vector3.new(0,3,0))		
