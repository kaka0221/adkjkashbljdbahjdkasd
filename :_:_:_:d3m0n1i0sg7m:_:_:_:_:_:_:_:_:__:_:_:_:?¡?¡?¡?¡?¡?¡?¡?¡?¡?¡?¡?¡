local Toggle = Featured:CreateToggle("Kill Gun", "Kill all players with the selected weapon", function(a) sound()
	if a then
		KillGunReal = true


			while KillGunReal do
				wait()
				for _,v in pairs(game.Players:GetChildren()) do
				pcall(function()
					
					if v ~= game.Players.LocalPlayer and (v.Character:FindFirstChild(ToolReal) or v.Backpack:FindFirstChild(ToolReal)) and v.Character:FindFirstChild('FULLY_LOADED_CHAR') and v.Name ~= ignoredplayer then
						local User = v.Name
						while getknife() ~= true do wait() end
						while game.Players:FindFirstChild(User).Character:FindFirstChild("FULLY_LOADED_CHAR") and game.Players:FindFirstChild(User) and (v.Character:FindFirstChild(ToolReal) or v.Backpack:FindFirstChild(ToolReal)) and User ~= nil do
							

							game:GetService("RunService").Stepped:wait()
-- Noclip 
local r15 = game.Players.LocalPlayer.Character

r15.Head.CanCollide = false
r15.UpperTorso.CanCollide = false
r15.LowerTorso.CanCollide = false
r15.LeftUpperArm.CanCollide = false
r15.LeftLowerArm.CanCollide = false
r15.RightUpperArm.CanCollide = false
r15.RightLowerArm.CanCollide = false
r15.LeftUpperLeg.CanCollide = false
r15.LeftLowerLeg.CanCollide = false
r15.RightUpperLeg.CanCollide = false
r15.RightLowerLeg.CanCollide = false
r15.HumanoidRootPart.CanCollide = false
							TargetPiso_.CFrame = CFrame.new(getprediction(User) + Vector3.new(0, - 13 ,0))

							if game.Players[User].Character.BodyEffects['K.O'].Value == true and game:GetService("Workspace").Players:WaitForChild(User):FindFirstChild("GRABBING_CONSTRAINT") == nil and game.Players[User].Character.BodyEffects['Dead'].Value == false then
							
								if TextRealWhenKill ~= nil and type(TextRealWhenKill) == "string" and TextRealWhenKill ~= '' then MessageChat(TextRealWhenKill) end

								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players[v.Name].Character.UpperTorso.Position + Vector3.new(0, 3, 0))	
	
								game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
								
							
							if game.Players[User].Character.BodyEffects['Dead'].Value == true or game.Players:FindFirstChild(v.Name) == nil then
								User = nil
							break
							end
								elseif game.Players[User].Character.BodyEffects['K.O'].Value == false and game.Players[User].Character.BodyEffects['Dead'].Value == false or game:GetService("Workspace").Players:WaitForChild(User):FindFirstChild("GRABBING_CONSTRAINT") == 'GRABBING_CONSTRAINT' then 
								pcall(function()
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TargetPiso_.Position + Vector3.new(0,3,0))
									pcall(function()game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["[Knife]"])
									
										game.Players.LocalPlayer.Character["[Knife]"].GripPos = Vector3.new(0, 5, 0)
										game.Players.LocalPlayer.Character["[Knife]"].Handle.Size = Vector3.new(50, 50, 50)
										end)
									game.Players.LocalPlayer.Character["[Knife]"]:Activate()
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TargetPiso_.Position + Vector3.new(0,3,0))
								end)
							elseif game.Players[User].Character.BodyEffects['Dead'].Value == true then
								game:GetService("Workspace").FallenPartsDestroyHeight = math.huge-math.huge
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-0.10987889766693115, -7996.8017578125, -0.1961374431848526)
							end
						end

					else
						game:GetService("Workspace").FallenPartsDestroyHeight = math.huge-math.huge
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-0.10987889766693115, -7996.8017578125, -0.1961374431848526)
								User = nil;
							end

				    end)
				end-- for end
		end

	else
		KillGunReal = false
	end

end)

