			game.Players.LocalPlayer.Character["[Knife]"].GripPos = Vector3.new(0, 5, 0)
			game.Players.LocalPlayer.Character["[Knife]"].Handle.Size = Vector3.new(50, 50, 50)
	    game.Players.LocalPlayer.Character["[Knife]"]:Activate()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TargetPiso_.Position + Vector3.new(0,3,0))	
		elseif game.Players[target].Character.BodyEffects['Dead'].Value == true then
			
			game:GetService("Workspace").FallenPartsDestroyHeight = math.huge-math.huge
			
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-0.10987889766693115, -7996.8017578125, -0.1961374431848526)

