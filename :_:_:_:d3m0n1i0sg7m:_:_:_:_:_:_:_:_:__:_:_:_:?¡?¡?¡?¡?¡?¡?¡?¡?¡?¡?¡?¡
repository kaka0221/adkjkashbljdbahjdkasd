			local A_1 = "Block"
			local A_2 = true
			local Event = game:GetService("ReplicatedStorage").MainEvent
			Event:FireServer(A_1, A_2)
		end)
		repeat wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-0.10987889766693115, -7996.8017578125, -0.1961374431848526)
			wait(0.005)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-102.03936004638672, -7996.8017578125, -400.9002685546875)
			wait(0.005)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(801.8656616210938, -7996.8017578125, 501.53289794921875)	
			wait(0.005)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(798.5054931640625, -7996.8017578125, -5002.3505859375)
			wait(0.005)
